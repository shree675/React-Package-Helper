    "name": "Npm Package Helper",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "activeTab"],
    "icons": {
        "16": "icons/npmicon16.png",
        "48": "icons/npmicon48.png",
        "128": "icons/npmicon128.png"
    }
}
